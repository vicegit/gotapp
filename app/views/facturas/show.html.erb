<div class="page-header">
  <%= link_to facturas_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_factura_path(@factura), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show factura</h1>
</div>

<dl class="dl-horizontal">
  <dt>Nrofact:</dt>
  <dd><%= @factura.nrofact %></dd>

  <dt>Cliente:</dt>
  <dd><%= @factura.cliente_id %></dd>

  <dt>Condicion:</dt>
  <dd><%= @factura.condicion %></dd>

  <dt>Fecha:</dt>
  <dd><%= @factura.fecha %></dd>

  <dt>Iva:</dt>
  <dd><%= @factura.iva %></dd>

  <dt>Erssan:</dt>
  <dd><%= @factura.erssan %></dd>

  <dt>Total:</dt>
  <dd><%= @factura.total %></dd>

</dl>
