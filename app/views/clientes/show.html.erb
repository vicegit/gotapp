<div class="page-header">
  <%= link_to clientes_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_cliente_path(@cliente), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show cliente</h1>
</div>

<dl class="dl-horizontal">
  <dt>Cod:</dt>
  <dd><%= @cliente.cod %></dd>

  <dt>Ci:</dt>
  <dd><%= @cliente.ci %></dd>

  <dt>Nombre:</dt>
  <dd><%= @cliente.nombre %></dd>

  <dt>Direccion:</dt>
  <dd><%= @cliente.direccion %></dd>

  <dt>Telefono:</dt>
  <dd><%= @cliente.telefono %></dd>

  <dt>Fecha contrato:</dt>
  <dd><%= @cliente.fecha_contrato %></dd>

  <dt>Fecha conexion:</dt>
  <dd><%= @cliente.fecha_conexion %></dd>

  <dt>Estado cuenta:</dt>
  <dd><%= @cliente.estado_cuenta %></dd>

  <dt>Observacion:</dt>
  <dd><%= @cliente.observacion %></dd>

</dl>
